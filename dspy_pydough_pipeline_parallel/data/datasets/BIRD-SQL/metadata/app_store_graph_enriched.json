[
  {
    "name": "app_store",
    "version": "V2",
    "collections": [
      {
        "name": "playstore",
        "type": "simple table",
        "table path": "main.playstore",
        "unique properties": [
          [
            "app",
            "category",
            "rating",
            "reviews",
            "size",
            "installs",
            "type_",
            "price",
            "content_rating",
            "genres"
          ]
        ],
        "properties": [
          {
            "name": "app",
            "type": "table column",
            "column name": "App",
            "data type": "string",
            "description": "The name of the application.",
            "sample values": [
              "Photo Editor & Candy Camera & Grid & ...",
              "Coloring book moana",
              "U Launcher Lite \u2013 FREE Live Cool Them...",
              "Sketch - Draw & Paint",
              "Pixel Draw - Number Art Coloring Book"
            ],
            "synonyms": [
              "App"
            ]
          },
          {
            "name": "category",
            "type": "table column",
            "column name": "Category",
            "data type": "string",
            "description": "The category to which the app belongs.",
            "sample values": [
              "ART_AND_DESIGN",
              "AUTO_AND_VEHICLES",
              "BEAUTY",
              "BOOKS_AND_REFERENCE",
              "BUSINESS"
            ],
            "synonyms": [
              "Category"
            ]
          },
          {
            "name": "rating",
            "type": "table column",
            "column name": "Rating",
            "data type": "numeric",
            "description": "The overall user rating of the app at the time of scraping.",
            "sample values": [
              4.1,
              3.9,
              4.7,
              4.5,
              4.3
            ],
            "synonyms": [
              "Rating"
            ]
          },
          {
            "name": "reviews",
            "type": "table column",
            "column name": "Reviews",
            "data type": "numeric",
            "description": "The number of user reviews for the app at the time of scraping.",
            "sample values": [
              159,
              967,
              87510,
              215644,
              167
            ],
            "synonyms": [
              "Reviews"
            ]
          },
          {
            "name": "size",
            "type": "table column",
            "column name": "Size",
            "data type": "string",
            "description": "The size of the app at the time of scraping. May vary with device.",
            "sample values": [
              "19M",
              "14M",
              "8.7M",
              "25M",
              "2.8M"
            ],
            "synonyms": [
              "Size"
            ]
          },
          {
            "name": "installs",
            "type": "table column",
            "column name": "Installs",
            "data type": "string",
            "description": "The number of user downloads or installs for the app at the time of scraping.",
            "sample values": [
              "10,000+",
              "500,000+",
              "5,000,000+",
              "50,000,000+",
              "100,000+"
            ],
            "synonyms": [
              "Installs"
            ]
          },
          {
            "name": "type_",
            "type": "table column",
            "column name": "Type",
            "data type": "string",
            "description": "Indicates if the app is 'Paid' or 'Free'.",
            "sample values": [
              "Free",
              "Paid",
              "NaN"
            ],
            "synonyms": [
              "Type"
            ]
          },
          {
            "name": "price",
            "type": "table column",
            "column name": "Price",
            "data type": "string",
            "description": "The price of the app at the time of scraping.",
            "sample values": [
              "0",
              "$4.99",
              "$3.99",
              "$6.99",
              "$1.49"
            ],
            "synonyms": [
              "Price"
            ],
            "extra semantic info": {
              "commonsense evidence": "A price of '0' indicates that the app is Free."
            }
          },
          {
            "name": "content_rating",
            "type": "table column",
            "column name": "\"Content Rating\"",
            "data type": "string",
            "description": "The age group the app is targeted at, such as Everyone, Teen, or Mature.",
            "sample values": [
              "Everyone",
              "Teen",
              "Everyone 10+",
              "Mature 17+",
              "Adults only 18+"
            ],
            "synonyms": [
              "Content Rating"
            ],
            "extra semantic info": {
              "wikipedia_age_groups": {
                "Children": "age < 12-13",
                "Teen": "13-19",
                "Adult/Mature": "21+"
              }
            }
          },
          {
            "name": "genres",
            "type": "table column",
            "column name": "Genres",
            "data type": "string",
            "description": "The genres an app belongs to. An app can have multiple genres apart from its main category.",
            "sample values": [
              "Art & Design",
              "Art & Design;Pretend Play",
              "Art & Design;Creativity",
              "Art & Design;Action & Adventure",
              "Auto & Vehicles"
            ],
            "synonyms": [
              "Genres"
            ]
          }
        ],
        "description": "Contains details and metadata of applications available on the Google Play Store.",
        "synonyms": [
          "playstore"
        ]
      },
      {
        "name": "user_reviews",
        "type": "simple table",
        "table path": "main.user_reviews",
        "unique properties": [
          [
            "app",
            "translated_review",
            "sentiment",
            "sentiment_polarity",
            "sentiment_subjectivity"
          ]
        ],
        "properties": [
          {
            "name": "app",
            "type": "table column",
            "column name": "App",
            "data type": "string",
            "description": "The name of the application being reviewed.",
            "sample values": [
              "10 Best Foods for You",
              "104 \u627e\u5de5\u4f5c - \u627e\u5de5\u4f5c \u627e\u6253\u5de5 \u627e\u517c\u8077 \u5c65\u6b77\u5065\u6aa2 \u5c65\u6b77\u8a3a\u7642\u5ba4",
              "11st",
              "1800 Contacts - Lens Store",
              "1LINE \u2013 One Line with One Touch"
            ],
            "synonyms": [
              "App"
            ]
          },
          {
            "name": "translated_review",
            "type": "table column",
            "column name": "Translated_Review",
            "data type": "string",
            "description": "The user's review, preprocessed and translated into English.",
            "sample values": [
              "I like eat delicious food. That's I'm...",
              "This help eating healthy exercise reg...",
              "nan",
              "Works great especially going grocery ...",
              "Best idea us"
            ],
            "synonyms": [
              "Translated_Review"
            ]
          },
          {
            "name": "sentiment",
            "type": "table column",
            "column name": "Sentiment",
            "data type": "string",
            "description": "The sentiment of the user review, categorized as 'Positive', 'Negative', or 'Neutral'.",
            "sample values": [
              "Positive",
              "nan",
              "Neutral",
              "Negative"
            ],
            "synonyms": [
              "Sentiment"
            ],
            "extra semantic info": {
              "sentiment_values": {
                "Positive": "User holds a positive attitude towards this app.",
                "Negative": "User holds a negative attitude or criticizes the app.",
                "Neutral": "User holds a neutral attitude.",
                "nan": "User did not provide a sentiment."
              }
            }
          },
          {
            "name": "sentiment_polarity",
            "type": "table column",
            "column name": "Sentiment_Polarity",
            "data type": "string",
            "description": "A score indicating the polarity of the sentiment in the review.",
            "sample values": [
              "1.0",
              "0.25",
              "nan",
              "0.4",
              "0.6000000000000001"
            ],
            "synonyms": [
              "Sentiment_Polarity"
            ],
            "extra semantic info": {
              "score_interpretation": {
                ">= 0.5": "Pretty positive attitude or strong liking.",
                "0 to < 0.5": "Mildly positive attitude.",
                "-0.5 to < 0": "Mildly negative attitude.",
                "< -0.5": "Strongly negative attitude or dislike."
              }
            }
          },
          {
            "name": "sentiment_subjectivity",
            "type": "table column",
            "column name": "Sentiment_Subjectivity",
            "data type": "string",
            "description": "A score indicating the subjectivity of the review.",
            "sample values": [
              "0.5333333333333333",
              "0.28846153846153844",
              "nan",
              "0.875",
              "0.3"
            ],
            "synonyms": [
              "Sentiment_Subjectivity"
            ],
            "extra semantic info": {
              "commonsense evidence": "A higher subjectivity score indicates less objectivity in the review, and vice versa."
            }
          }
        ],
        "description": "Contains user reviews for applications, including the review text and sentiment analysis.",
        "synonyms": [
          "user_reviews"
        ]
      }
    ],
    "relationships": [
      {
        "type": "simple join",
        "name": "user_reviews",
        "parent collection": "playstore",
        "child collection": "user_reviews",
        "singular": false,
        "always matches": false,
        "keys": {
          "app": [
            "app"
          ]
        },
        "description": "Retrieves all user reviews associated with a specific application.",
        "synonyms": []
      },
      {
        "type": "reverse",
        "name": "playstore",
        "original parent": "playstore",
        "original property": "user_reviews",
        "singular": false,
        "always matches": false,
        "description": "Retrieves the application details associated with a specific user review.",
        "synonyms": []
      }
    ]
  }
]