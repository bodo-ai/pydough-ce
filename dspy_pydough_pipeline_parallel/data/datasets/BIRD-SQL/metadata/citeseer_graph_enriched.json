[
  {
    "name": "citeseer",
    "version": "V2",
    "collections": [
      {
        "name": "cites",
        "type": "simple table",
        "table path": "main.cites",
        "unique properties": [
          [
            "cited_paper_id",
            "citing_paper_id"
          ]
        ],
        "properties": [
          {
            "name": "cited_paper_id",
            "type": "table column",
            "column name": "cited_paper_id",
            "data type": "string",
            "description": "The ID of the paper being cited.",
            "sample values": [
              "100157",
              "100598",
              "101570",
              "10227",
              "103027"
            ],
            "synonyms": [
              "cited_paper_id"
            ]
          },
          {
            "name": "citing_paper_id",
            "type": "table column",
            "column name": "citing_paper_id",
            "data type": "string",
            "description": "The ID of the paper which contains the citation.",
            "sample values": [
              "100157",
              "364207",
              "38848",
              "bradshaw97introduction",
              "bylund99coordinating"
            ],
            "synonyms": [
              "citing_paper_id"
            ]
          }
        ],
        "description": "A collection of citations, linking papers that cite other papers.",
        "synonyms": [
          "cites"
        ]
      },
      {
        "name": "content",
        "type": "simple table",
        "table path": "main.content",
        "unique properties": [
          [
            "paper_id",
            "word_cited_id"
          ]
        ],
        "properties": [
          {
            "name": "paper_id",
            "type": "table column",
            "column name": "paper_id",
            "data type": "string",
            "description": "Unique string ID of the paper.",
            "sample values": [
              "100157",
              "100598",
              "101570",
              "10227",
              "102637"
            ],
            "synonyms": [
              "paper_id"
            ]
          },
          {
            "name": "word_cited_id",
            "type": "table column",
            "column name": "word_cited_id",
            "data type": "string",
            "description": "ID of a word from the vocabulary.",
            "sample values": [
              "word1163",
              "word1509",
              "word1614",
              "word1642",
              "word1663"
            ],
            "synonyms": [
              "word_cited_id"
            ]
          }
        ],
        "description": "Represents the content of each paper as a bag-of-words. Each row indicates that a specific word is present in a specific paper.",
        "synonyms": [
          "content"
        ],
        "extra semantic info": {
          "commonsense evidence": "Each row in this table indicates that the word identified by word_cited_id is present in the paper identified by paper_id."
        }
      },
      {
        "name": "paper",
        "type": "simple table",
        "table path": "main.paper",
        "unique properties": [
          "paper_id"
        ],
        "properties": [
          {
            "name": "paper_id",
            "type": "table column",
            "column name": "paper_id",
            "data type": "string",
            "description": "Unique string ID of the paper.",
            "sample values": [
              "100157",
              "100598",
              "101570",
              "10227",
              "102637"
            ],
            "synonyms": [
              "paper_id"
            ]
          },
          {
            "name": "class_label",
            "type": "table column",
            "column name": "class_label",
            "data type": "string",
            "description": "The subject class or category of the paper.",
            "sample values": [
              "Agents",
              "IR",
              "ML",
              "AI",
              "DB"
            ],
            "synonyms": [
              "class_label"
            ]
          }
        ],
        "description": "A collection of papers, each identified by a unique ID and classified into a subject area.",
        "synonyms": [
          "paper"
        ]
      }
    ],
    "relationships": [
      {
        "type": "simple join",
        "name": "content",
        "parent collection": "paper",
        "child collection": "content",
        "singular": false,
        "always matches": false,
        "keys": {
          "paper_id": [
            "paper_id"
          ]
        },
        "description": "The set of words contained in this paper.",
        "synonyms": []
      },
      {
        "type": "reverse",
        "name": "paper",
        "original parent": "paper",
        "original property": "content",
        "singular": true,
        "always matches": true,
        "description": "The paper that contains this word.",
        "synonyms": []
      },
      {
        "type": "simple join",
        "name": "outgoing_citations",
        "parent collection": "paper",
        "child collection": "cites",
        "singular": false,
        "always matches": false,
        "keys": {
          "paper_id": [
            "citing_paper_id"
          ]
        },
        "description": "The list of citations made by this paper.",
        "synonyms": []
      },
      {
        "type": "reverse",
        "name": "citing_paper",
        "original parent": "paper",
        "original property": "outgoing_citations",
        "singular": true,
        "always matches": true,
        "description": "The paper that contains this citation.",
        "synonyms": []
      },
      {
        "type": "simple join",
        "name": "incoming_citations",
        "parent collection": "paper",
        "child collection": "cites",
        "singular": false,
        "always matches": false,
        "keys": {
          "paper_id": [
            "cited_paper_id"
          ]
        },
        "description": "The list of citations this paper has received.",
        "synonyms": []
      },
      {
        "type": "reverse",
        "name": "cited_paper",
        "original parent": "paper",
        "original property": "incoming_citations",
        "singular": true,
        "always matches": true,
        "description": "The paper that is being cited.",
        "synonyms": []
      }
    ]
  }
]