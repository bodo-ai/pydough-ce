[
  {
    "name": "mental_health_survey",
    "version": "V2",
    "collections": [
      {
        "name": "answer",
        "type": "simple table",
        "table path": "main.Answer",
        "unique properties": [
          [
            "userid",
            "questionid"
          ]
        ],
        "properties": [
          {
            "name": "answertext",
            "type": "table column",
            "column name": "AnswerText",
            "data type": "string",
            "description": "The text content of the answer provided by a user.",
            "sample values": [
              "37",
              "44",
              "32",
              "31",
              "33"
            ],
            "synonyms": [
              "AnswerText"
            ]
          },
          {
            "name": "surveyid",
            "type": "table column",
            "column name": "SurveyID",
            "data type": "numeric",
            "description": "Identifier for the survey to which the answer belongs. Foreign key to the survey collection.",
            "sample values": [
              2014,
              2016,
              2017,
              2018,
              2019
            ],
            "synonyms": [
              "SurveyID"
            ],
            "extra semantic info": {
              "format": "The SurveyID corresponds to the year the survey was conducted."
            }
          },
          {
            "name": "userid",
            "type": "table column",
            "column name": "UserID",
            "data type": "numeric",
            "description": "Identifier for the user who provided the answer.",
            "sample values": [
              1,
              2,
              3,
              4,
              5
            ],
            "synonyms": [
              "UserID"
            ]
          },
          {
            "name": "questionid",
            "type": "table column",
            "column name": "QuestionID",
            "data type": "numeric",
            "description": "Identifier for the question being answered. Foreign key to the question collection.",
            "sample values": [
              1,
              2,
              3,
              4,
              5
            ],
            "synonyms": [
              "QuestionID"
            ]
          }
        ],
        "description": "Stores individual answers from users to specific questions within a survey.",
        "synonyms": [
          "Answer"
        ],
        "extra semantic info": {
          "commonsense evidence": "Some questions can contain multiple answers, thus the same user can appear more than once for that QuestionID."
        }
      },
      {
        "name": "question",
        "type": "simple table",
        "table path": "main.Question",
        "unique properties": [
          "questionid"
        ],
        "properties": [
          {
            "name": "questiontext",
            "type": "table column",
            "column name": "questiontext",
            "data type": "string",
            "description": "The full text of the survey question.",
            "sample values": [
              "What is your age?",
              "What is your gender?",
              "What country do you live in?",
              "If you live in the United States, whi...",
              "Are you self-employed?"
            ],
            "synonyms": [
              "questiontext"
            ]
          },
          {
            "name": "questionid",
            "type": "table column",
            "column name": "questionid",
            "data type": "numeric",
            "description": "The unique identifier for a question.",
            "sample values": [
              1,
              2,
              3,
              4,
              5
            ],
            "synonyms": [
              "questionid"
            ]
          }
        ],
        "description": "Contains the list of all unique questions asked across all surveys.",
        "synonyms": [
          "Question"
        ]
      },
      {
        "name": "survey",
        "type": "simple table",
        "table path": "main.Survey",
        "unique properties": [
          "surveyid"
        ],
        "properties": [
          {
            "name": "surveyid",
            "type": "table column",
            "column name": "SurveyID",
            "data type": "numeric",
            "description": "The unique identifier for a survey.",
            "sample values": [
              2014,
              2016,
              2017,
              2018,
              2019
            ],
            "synonyms": [
              "SurveyID"
            ],
            "extra semantic info": {
              "format": "The SurveyID is the year the survey was conducted and is unique for each survey."
            }
          },
          {
            "name": "description",
            "type": "table column",
            "column name": "Description",
            "data type": "string",
            "description": "A brief description of the survey.",
            "sample values": [
              "mental health survey for 2014",
              "mental health survey for 2016",
              "mental health survey for 2017",
              "mental health survey for 2018",
              "mental health survey for 2019"
            ],
            "synonyms": [
              "Description"
            ]
          }
        ],
        "description": "Contains information about the different surveys conducted, identified by year.",
        "synonyms": [
          "Survey"
        ]
      }
    ],
    "relationships": [
      {
        "type": "simple join",
        "name": "answers",
        "parent collection": "question",
        "child collection": "answer",
        "singular": false,
        "always matches": false,
        "keys": {
          "questionid": [
            "questionid"
          ]
        },
        "description": "The set of answers given for this question.",
        "synonyms": []
      },
      {
        "type": "reverse",
        "name": "question",
        "original parent": "question",
        "original property": "answers",
        "singular": true,
        "always matches": false,
        "description": "The question that this answer corresponds to.",
        "synonyms": []
      },
      {
        "type": "simple join",
        "name": "answers",
        "parent collection": "survey",
        "child collection": "answer",
        "singular": false,
        "always matches": false,
        "keys": {
          "surveyid": [
            "surveyid"
          ]
        },
        "description": "The set of answers collected in this survey.",
        "synonyms": []
      },
      {
        "type": "reverse",
        "name": "survey",
        "original parent": "survey",
        "original property": "answers",
        "singular": true,
        "always matches": false,
        "description": "The survey in which this answer was given.",
        "synonyms": []
      }
    ]
  }
]