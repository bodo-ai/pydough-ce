[
  {
    "name": "music_tracker",
    "version": "V2",
    "collections": [
      {
        "name": "tags",
        "type": "simple table",
        "table path": "main.tags",
        "unique properties": [
          "index_"
        ],
        "properties": [
          {
            "name": "index_",
            "type": "table column",
            "column name": "\"index\"",
            "data type": "numeric",
            "description": "The index of the tag record.",
            "sample values": [
              0,
              1,
              2,
              3,
              4
            ],
            "synonyms": [
              "index"
            ]
          },
          {
            "name": "id_",
            "type": "table column",
            "column name": "id",
            "data type": "numeric",
            "description": "Release identifier which can be matched with the 'id_' field in the 'torrents' table.",
            "sample values": [
              0,
              2,
              3,
              4,
              6
            ],
            "synonyms": [
              "id"
            ]
          },
          {
            "name": "tag",
            "type": "table column",
            "column name": "tag",
            "data type": "string",
            "description": "The tag associated with a music release.",
            "sample values": [
              "1970s",
              "funk",
              "disco",
              "new.york",
              "united.states"
            ],
            "synonyms": [
              "tag"
            ]
          }
        ],
        "description": "Stores tags associated with music releases.",
        "synonyms": [
          "tags"
        ]
      },
      {
        "name": "torrents",
        "type": "simple table",
        "table path": "main.torrents",
        "unique properties": [
          "id_"
        ],
        "properties": [
          {
            "name": "groupname",
            "type": "table column",
            "column name": "groupName",
            "data type": "string",
            "description": "The title of the music release.",
            "sample values": [
              "superappin&#39;",
              "spiderap / a corona jam",
              "rapper&#39;s delight",
              "rap-o clap-o / el rap-o clap-o",
              "christmas rappin&#39;"
            ],
            "synonyms": [
              "groupName"
            ]
          },
          {
            "name": "totalsnatched",
            "type": "table column",
            "column name": "totalSnatched",
            "data type": "numeric",
            "description": "The number of times the release has been downloaded.",
            "sample values": [
              239,
              156,
              480,
              200,
              109
            ],
            "synonyms": [
              "totalSnatched"
            ]
          },
          {
            "name": "artist",
            "type": "table column",
            "column name": "artist",
            "data type": "string",
            "description": "The name of the artist or group.",
            "sample values": [
              "grandmaster flash & the furious five",
              "ron hunt & ronnie g & the sm crew",
              "sugarhill gang",
              "joe bataan",
              "kurtis blow"
            ],
            "synonyms": [
              "artist"
            ]
          },
          {
            "name": "groupyear",
            "type": "table column",
            "column name": "groupYear",
            "data type": "numeric",
            "description": "The year the music was released.",
            "sample values": [
              1979,
              1980,
              1981,
              1982,
              1983
            ],
            "synonyms": [
              "groupYear"
            ]
          },
          {
            "name": "releasetype",
            "type": "table column",
            "column name": "releaseType",
            "data type": "string",
            "description": "The type of release (e.g., album, single, mixtape).",
            "sample values": [
              "single",
              "album",
              "live album",
              "ep",
              "unknown"
            ],
            "synonyms": [
              "releaseType"
            ]
          },
          {
            "name": "groupid",
            "type": "table column",
            "column name": "groupId",
            "data type": "numeric",
            "description": "Unique release identifier used to ensure no releases are duplicates.",
            "sample values": [
              720949,
              728752,
              18513,
              756236,
              71818958
            ],
            "synonyms": [
              "groupId"
            ],
            "extra semantic info": {
              "source": "What.CD"
            }
          },
          {
            "name": "id_",
            "type": "table column",
            "column name": "id",
            "data type": "numeric",
            "description": "A unique identifier for the release, essentially an index.",
            "sample values": [
              0,
              1,
              2,
              3,
              4
            ],
            "synonyms": [
              "id"
            ]
          }
        ],
        "description": "Contains information about music releases, including artist, title, and download statistics.",
        "synonyms": [
          "torrents"
        ]
      }
    ],
    "relationships": [
      {
        "type": "simple join",
        "name": "tags",
        "parent collection": "torrents",
        "child collection": "tags",
        "singular": false,
        "always matches": false,
        "keys": {
          "id_": [
            "id_"
          ]
        },
        "description": "The tags associated with this torrent release.",
        "synonyms": []
      },
      {
        "type": "reverse",
        "name": "torrent",
        "original parent": "torrents",
        "original property": "tags",
        "singular": true,
        "always matches": false,
        "description": "The torrent release associated with this tag.",
        "synonyms": []
      }
    ]
  }
]